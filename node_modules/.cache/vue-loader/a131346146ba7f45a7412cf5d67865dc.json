button\">\n    <span><slot></slot></span>\n  </button>\n</template>\n\n<script>\nexport default {\n  name: 'dos-button'\n}\n</script>\n<style>\n.dos-button {\n  display: inline-block;\n  line-height: 1;\n  white-space: nowrap;\n  cursor: pointer;\n  background: #ffffff;\n  border: 1px solid #dcdfe6;\n  color: #606266;\n  -webkit-appearance: none;\n  text-align: center;\n  box-sizing: border-box;\n  outline: none;\n  margin: 0;\n  transition: 0.1s;\n  font-weight: 500;\n  -moz-user-select: none;\n  -webkit-user-select: none;\n  -moz-user-select: none;\n  -ms-user-select: none;\n  padding: 12px 20px;\n  font-size: 14px;\n  border-radius: 4px;\n\n&\n:hover,\n\n&\n:focus {\n  color: #409eff;\n  border-color: #c6e2ff;\n  background-color: #ecf5ff;\n}\n\n}\n.dos-button-primary {\n  color: #fff;\n  background-color: #409eff;\n  border-color: #409eff;\n\n&\n:hover,\n\n&\n:focus {\n  background: #66b1ff;\n  background-color: #66b1ff;\n  color: #fff;\n}\n\n}\n.dos-button-success {\n  color: #fff;\n  background-color: #67c23a;\n  border-color: #67c23a;\n\n&\n:hover,\n\n&\n:focus {\n  background: #85ce61;\n  background-color: #85ce61;\n  color: #fff;\n}\n\n}\n.dos-button-info {\n  color: #fff;\n  background-color: #909399;\n  border-color: #909399;\n\n&\n:hover,\n\n&\n:focus {\n  background: #a6a9ad;\n  background-color: #a6a9ad;\n  color: #fff;\n}\n\n}\n.dos-button-warning {\n  color: #fff;\n  background-color: #e6a23c;\n  border-color: #e6a23c;\n\n&\n:hover,\n\n&\n:focus {\n  background: #ebb563;\n  background-color: #ebb563;\n  color: #fff;\n}\n\n}\n.dos-button-danger {\n  color: #fff;\n  background-color: #f56c6c;\n  border-color: #f56c6c;\n\n&\n:hover,\n\n&\n:focus {\n  background: #f78989;\n  background-color: #f78989;\n  color: #fff;\n}\n\n}\n\n.dos-button.is-plain {\n\n&\n:hover,\n\n&\n:focus {\n  background: #fff;\n  border-color: #489eff;\n  color: #409eff;\n}\n\n}\n.dos-button-primary.is-plain {\n  color: #409eff;\n  background: #ecf5ff;\n\n&\n:hover,\n\n&\n:focus {\