export default {\n  name: \"dos-input\",\n  data() {\n    return {\n      passwordVisible: false,\n    };\n  },\n  props: {\n    placeholder: {\n      type: String,\n      default: \"\",\n    },\n    type: {\n      type: String,\n      default: \"text\",\n    },\n    name: {\n      type: String,\n      default: \"\",\n    },\n    disabled: {\n      type: Boolean,\n      default: false,\n    },\n    value: {\n      type: String,\n    },\n    clearable: {\n      type: Boolean,\n      default: false,\n    },\n    showPassword: {\n      type: Boolean,\n      default: false,\n    },\n  },\n  computed: {\n    showSuffix() {\n      return this.clearable || this.showPassword;\n    },\n  },\n  methods: {\n    handleInput(e) {\n      // 父组件的v-model中已经包含了@input事件，因此\n      this.$emit(\"input\", e.target.value);\n    },\n    clear() {\n      this.$emit(\"input\", \"\");\n    },\n    handlePassword() {\n      this.passwordVisible = !this.passwordVisible;\n    },\n  },\n};\n</script>\n\n<style lang=\"scss\" scoped>\n.dos-input {\n  width: 100%;\n  position: relative;\n  font-size: 14px;\n  display: inline-block;\n  .dos-input_inner {\n    -webkit-appearance: none;\n    background-color: #fff;\n    background-image: none;\n    border: 2px solid #a5a5a5;\n    box-sizing: border-box;\n    color: #3c3c3d;\n    display: inline-block;\n    font-size: inherit;\n    height: 30px;\n    line-height: 30px;\n    outline: none;\n    padding: 0 10px;\n    transition: border-color 0.2s cubic-bezier(0.645, 045, 0.355, 1);\n    width: 100%;\n    &:focus {\n      outline: none;\n      border-color: #696c6c;\n    }\n    &.is-disabled {\n      background-color: #f5f7fa;\n      border-color: #e4e7ed;\n      color: #c0c4cc;\n      cursor: not-allowed;\n    }\n  }\n}\n// 后面加suffix的意思是后面如果有后缀的话，触发该样式\n.dos-input__suffix {\n  .dos-input_inner {\n    padding-right: 30px;\n  }\n\n  .dos-input_suffix {\n    position: absolute;\n    right: 10px;\n    height: 100%;\n    top: 0;\n    line-height: 35px;\n    text-align: center;\n    color: #c0c4cc;\n    transition: all 0.3s;\n    z-index: 900;\n\n    i {\n      color: #c0c4cc;\n      font-size: 14px;\n      cursor: pointer;\n      transition: color 0.2s cubic-bezier(0.645, 0.045, 0.355, 1);\n    }\n  }\n}\n</style>\n"]}]}