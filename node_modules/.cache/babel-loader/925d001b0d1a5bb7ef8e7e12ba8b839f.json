le: {\n      type: Boolean,\n      default: false\n    },\n  },\n  methods: {\n    // 定义一个点击事件，这个点击事件的作用是调用父组件中的点击事件，并且回调\n    handleClick (e) {\n      this.$emit('click', e)\n    }\n  }\n};\n</script>\n<style lang=\"scss\" scoped>\n.dos-button {\n  display: inline-block;\n  line-height: 1;\n  white-space: nowrap;\n  cursor: pointer;\n  background: #ffffff;\n  border: 0px solid #dcdfe6;\n  color: #606266;\n  -webkit-appearance: none;\n  text-align: center;\n  box-sizing: border-box;\n  outline: none;\n  margin: 0;\n  transition: 0.25s;\n  font-weight: 500;\n  -moz-user-select: none;\n  -webkit-user-select: none;\n  -moz-user-select: none;\n  -ms-user-select: none;\n  padding: 8px 12px;\n  font-size: 14px;\n  &:hover,\n  &:focus {\n    background: rgb(248, 248, 255);\n    transition: ease 0.5s;\n  }\n}\n\n  .dos-button-primary{\n  color:#fff;\n  background-color: #022a58;\n  &:hover,\n  &:focus{\n    background: #66b1ff;\n    background-color: #66b1ff;\n    color: #fff;\n    }\n  }\n  .dos-button-success{\n  color:#fff;\n  background-color: #59a137;\n  &:hover,\n  &:focus{\n    background: #85ce61;\n    background-color: #85ce61;\n    color: #fff;\n    }\n  }\n  .dos-button-info{\n  color:#fff;\n  background-color: #595c5c;\n  &:hover,\n  &:focus{\n    background: #a6a9ad;\n    background-color: #a6a9ad;\n    color: #fff;\n    }\n  }\n  .dos-button-warning{\n  color:#fff;\n  background-color: #e87d2b;\n  &:hover,\n  &:focus{\n    background: #ebb563;\n    background-color: #ebb563;\n    color: #fff;\n    }\n  }\n  .dos-button-danger{\n  color:#fff;\n  background-color: #ad0808;\n  &:hover,\n  &:focus{\n    background: #f78989;\n    background-color: #f78989;\n    color: #fff;\n    }\n  }\n\n.dos-button.is-plain{\n  &:hover,\n  &:focus{\n    background: #030982;\n    color: #d4e2f6;\n  }\n}\n.dos-button-primary.is-plain{\n  color: #0d62ba;\n  background: #ecf5ff;\n  &:hover,\n  &:focus{\n    background: #042545;\n    color: #fff;\n  }\n}\n.dos-button-success.is-plain{\n  color: #67c23a;\n  background: #c2e7b0;\n  &:hover,\n  &:focus{\n    background: #5db431;\n    color: #fff;\n  }\n}\n.dos-button-info.is-plain{\n  color: #909399;\n  background: #d3d4d6;\n  &:hover,\n  &:focus{\n    background: #636262;\n    color: #fff;\n  }\n}\n.dos-button-warning.is-plain{\n  color: #ea6733;\n  background: #f5dab1;\n  &:hover,\n  &:focus{\n    background: #e6a23c;\n    color: #fff;\n  }\n}\n.dos-button-danger.is-plain{\n  color: #f56c6c;\n  background: #fbc4c4;\n  &:hover,\n  &:focus{\n    background: #ac1010;\n    color: #fff;\n  }\n}\n// round属性\n.dos-button.is-round{\n  border-radius: 20px;\n  padding: 10px 18px;\n}\n// circle属性\n.dos-button.is-circle{\n  border-radius: 50%;\n  padding: 12px;\n}\n// icon配套样式\n.dos-button [class*=one-icon-]+span{\n  margin-left: 5px;\n}\n// disabled属性\n.dos-button.is-disabled{\n   cursor: no-drop;\n}\n\n</style>"],"sourceRoot":"src/components"}]}